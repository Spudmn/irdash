{
  "private": true,
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "build --dist --platform=win32 --arch=all",
    "electron": "electron --enable-logging app"
  },
  "build": {
    "asar": true,
    "compression": "store",
    "win": {
      "iconUrl": "https://github.com/electron/electron/raw/master/atom/browser/resources/win/atom.ico",
      "noMsi": false
    }
  },
  "directories": {
      "buildResources": "build",
      "output": "dist",
      "app": "app"
  },
  "devDependencies": {
    "angular": "^1.5.5",
    "angular-route": "^1.5.5",
    "angular-websocket": "^1.1.0",
    "asar": "^0.11.0",
    "babel-core": "^6.7.7",
    "babel-preset-angular": "^6.0.15",
    "babel-preset-es2015": "^6.6.0",
    "del": "^2.2.0",
    "electron-builder": "^3.17.1",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^0.37.6",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.7",
    "gulp-concat": "^2.6.0",
    "gulp-less": "^3.0.5",
    "gulp-plumber": "^1.1.0",
    "gulp-uglify": "^1.5.3",
    "less": "^2.6.1"
  }
}
